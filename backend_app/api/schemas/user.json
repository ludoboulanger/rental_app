{
  "requires": ["firstName", "lastName", "email", "phoneNumber", "password"],
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "maxLength": 50
    },
    "lastName": {
      "type": "string",
      "maxLength": 50
    },
    "email": {
      "type": "string",
      "format": "email",
      "maxLength": 50
    },
    "phoneNumber": {
      "type": "string",
      "pattern": "^\\+[1-9]\\d{10,14}$",
      "maxLength": 50,
      "minLength": 1
    },
    "password": {
      "type": "string",
      "maxLength": 100
    }
  }
}